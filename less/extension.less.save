// cursor hover type from: https://codepen.io/FlorianLudwig/pen/zKzPLq

@import 'https://fonts.googleapis.com/css?family=Open+Sans';

* {
  font-family: 'Open Sans', sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  margin: 0 auto;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;  
}



.ttwrapper{
  position:absolute;
  left:0;
  right:0;
}

#ttemail{
  font-size:24px;
	color: black;
	letter-spacing:.25px;
	font-weight:700;
}

#ttemail .ttcursor{
	-webkit-transition: all 600ms cubic-bezier(0.23, 1, 0.32, 1);
  transition:         all 600ms cubic-bezier(0.23, 1, 0.32, 1);
	animation: blinker 1s linear infinite;
  color:#f8c332;
}

#ttemail:hover .ttbutton{
	opacity:0;
}

#ttemail p {
  width: 0em;
  white-space:nowrap;
  overflow:hidden;
	display:inline-block;
	bottom:-8px;
	position:relative;
}
#ttemail a{
	display:inline-block;
	-webkit-transition: all 200ms cubic-bezier(.23, 1, .32, 1);
  transition:         all 200ms cubic-bezier(.23, 1, .32, 1);
  color: black;
  text-decoration:none;
}

#ttemail span{
	display:inline-block;
	position:relative;
}

#ttemail:hover p {
  width: 100%;
  white-space:nowrap;
  overflow:hidden;
  -webkit-animation: type 1s steps(40, end);
  animation: type 1s steps(40, end);
text-decoration: none;
}

/*KEYFRAME ANIMATION*/
@keyframes type{
  from { width: 0; }
}

@-webkit-keyframes type{
  from { width: 0; }
}

@keyframes blinker {
  50% { opacity: 0.0; }
}

@-webkit-keyframes blinker{
	50% { opacity: 0.0; }
}

@-moz-keyframes blinker{
	50% { opacity: 0.0; }
}

@-o-keyframes blinker{
	50% { opacity: 0.0; }
}


